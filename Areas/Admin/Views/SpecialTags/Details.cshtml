@model Core_MVC_Store.Models.SpecialTags
@{
    ViewData["Title"] = "Details";
}



<div class="row mt-1">
    <div class="col">
        <h2 class="text-info text-center">Special Tag Details</h2>
    </div>
</div>
<div class=" row mt-1">
    <div class="col">

        <div class="p-4 border rounded">
            <div class="row mt-1 form-group">
                <div class="col-1 mt-1">
                    <p>ID</p>
                </div>
                <div class="col-1">
                    <input class="form-control" disabled="disabled" asp-for="Id"/>
                </div>
                <div class="col-1 mt-1">
                    <label asp-for="Name"></label>
                </div>
                <div class="col-5">
                    <input class="form-control" disabled="disabled" asp-for="Name"/>
                </div>
            </div>
            <div class="form-group">
                <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Edit</a>
                <a class="btn btn-success" asp-action="Index">Back to list</a>
            </div>

        </div>
 
    </div>
</div>